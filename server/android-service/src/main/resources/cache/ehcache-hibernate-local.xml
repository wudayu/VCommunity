    <cache name="com.vcommunity.android.entity.PropertyCompanyEntity" maxEntriesLocalHeap="1000" eternal="true"
           overflowToDisk="true" maxEntriesLocalDisk="10000" />
    <cache name="com.vcommunity.android.entity.PropertyEntity" maxEntriesLocalHeap="1000" eternal="true"
           overflowToDisk="true" maxEntriesLocalDisk="10000" />
</ehcache>